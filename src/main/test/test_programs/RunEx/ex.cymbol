void main() {
	int x;
	x = input();
	while(1){
		int y;
		if((x>0)&&(x<50)){
			x = x - 30;
			if(x<10){
				x = 2 * x;
				if(x<5){
					y = x + 10;
				}
				else{
					y = x;
				}
			}
			else{
				x = x + 2;
			}
			
		}
		else{
			y = x - 10;
		}
		if(x > 60){
			y = x;
		}
		else{
			y = x + 10;
		}
		x = x + 1;
	}

}

